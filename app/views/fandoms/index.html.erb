<p id="notice"><%= notice %></p>

<h1>Fandoms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Subtitle</th>
      <th>Description</th>
      <th>Profile img</th>
      <th>Bg img</th>
      <th>Admin</th>
      <th>Artist</th>
      <th>Group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fandoms.each do |fandom| %>
      <tr>
        <td><%= fandom.name %></td>
        <td><%= fandom.title %></td>
        <td><%= fandom.subtitle %></td>
        <td><%= fandom.description %></td>
        <td><%= fandom.profile_img %></td>
        <td><%= fandom.bg_img %></td>
        <td><%= fandom.admin_id %></td>
        <td><%= fandom.artist %></td>
        <td><%= fandom.group %></td>
        <td><%= link_to 'Show', fandom %></td>
        <td><%= link_to 'Edit', edit_fandom_path(fandom) %></td>
        <td><%= link_to 'Destroy', fandom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fandom', new_fandom_path %>
